	month = aug,
	year = {2021},
	keywords = {BCD Screen I - Exclude},
	pages = {e2119100},
	file = {A Data Set and Deep Learning Algorithm for the Detection of Masses and Architectural Distortions in Digital Breast Tomosynthesis Images.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\A Data Set and Deep Learning Algorithm for the Detection of Masses and Architectural Distortions in Digital Breast Tomosynthesis Images.md:text/plain;Buda et al. - 2021 - A Data Set and Deep Learning Algorithm for the Det.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\W8DEZGIN\\Buda et al. - 2021 - A Data Set and Deep Learning Algorithm for the Det.pdf:application/pdf},
