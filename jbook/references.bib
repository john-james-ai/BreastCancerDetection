	pages = {33},
	file = {How to develop a meaningful radiomic signature for clinical use in oncologic patients.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\How to develop a meaningful radiomic signature for clinical use in oncologic patients.md:text/plain;How to develop a meaningful radiomic signature for clinical use in oncologic patients.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\How to develop a meaningful radiomic signature for clinical use in oncologic patients.md:text/plain;Papanikolaou et al. - 2020 - How to develop a meaningful radiomic signature for.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\ESFM4XSM\\Papanikolaou et al. - 2020 - How to develop a meaningful radiomic signature for.pdf:application/pdf},
