	month = mar,
	year = {2016},
	note = {arXiv:1603.05955 [cs]},
	keywords = {Computer Science - Computer Vision and Pattern Recognition},
	file = {Transferring Learned Microcalcification Group Detection from 2D Mammography to 3D Digital Breast Tomosynthesis Using a Hierarchical Model and Scope-based Normalization Features.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\Transferring Learned Microcalcification Group Detection from 2D Mammography to 3D Digital Breast Tomosynthesis Using a Hierarchical Model and Scope-based Normalization Features.md:text/plain;Transferring Learned Microcalcification Group Detection from 2D Mammography to 3D Digital Breast Tomosynthesis Using a Hierarchical Model and Scope-based Normalization Features.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\Transferring Learned Microcalcification Group Detection from 2D Mammography to 3D Digital Breast Tomosynthesis Using a Hierarchical Model and Scope-based Normalization Features.md:text/plain;Yin et al. - 2016 - Transferring Learned Microcalcification Group Dete.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\CWU3IX23\\Yin et al. - 2016 - Transferring Learned Microcalcification Group Dete.pdf:application/pdf},
