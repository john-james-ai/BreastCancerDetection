	pages = {749--762},
	file = {Lambin et al. - 2017 - Radiomics the bridge between medical imaging and .pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\HVEXT2A5\\Lambin et al. - 2017 - Radiomics the bridge between medical imaging and .pdf:application/pdf;Radiomics the bridge between medical imaging and personalized medicine.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\Radiomics the bridge between medical imaging and personalized medicine.md:text/plain;Radiomics the bridge between medical imaging and personalized medicine.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\Radiomics the bridge between medical imaging and personalized medicine.md:text/plain},
