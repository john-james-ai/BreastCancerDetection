	pages = {110--118},
	file = {Arefan et al. - 2020 - Deep learning modeling using normal mammograms for.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\3XIZ3FA8\\Arefan et al. - 2020 - Deep learning modeling using normal mammograms for.pdf:application/pdf;Deep learning modeling using normal mammograms for predicting breast cancer risk.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\Deep learning modeling using normal mammograms for predicting breast cancer risk.md:text/plain;Deep learning modeling using normal mammograms for predicting breast cancer risk.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\Deep learning modeling using normal mammograms for predicting breast cancer risk.md:text/plain},
