	file = {Synthesizing lesions using contextual GANs improves breast cancer classification on mammograms.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\Synthesizing lesions using contextual GANs improves breast cancer classification on mammograms.md:text/plain;Synthesizing lesions using contextual GANs improves breast cancer classification on mammograms.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\Synthesizing lesions using contextual GANs improves breast cancer classification on mammograms.md:text/plain;Wu et al. - 2020 - Synthesizing lesions using contextual GANs improve.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\S7GTBN6E\\Wu et al. - 2020 - Synthesizing lesions using contextual GANs improve.pdf:application/pdf},
