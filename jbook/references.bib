	month = jun,
	year = {2018},
	note = {arXiv:1703.07047 [cs, stat]},
	keywords = {Computer Science - Machine Learning, Statistics - Machine Learning, Computer Science - Computer Vision and Pattern Recognition},
	file = {Geras et al. - 2018 - High-Resolution Breast Cancer Screening with Multi.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\MBYA9D6Z\\Geras et al. - 2018 - High-Resolution Breast Cancer Screening with Multi.pdf:application/pdf;High-Resolution Breast Cancer Screening with Multi-View Deep Convolutional Neural Networks.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\High-Resolution Breast Cancer Screening with Multi-View Deep Convolutional Neural Networks.md:text/plain;High-Resolution Breast Cancer Screening with Multi-View Deep Convolutional Neural Networks.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\High-Resolution Breast Cancer Screening with Multi-View Deep Convolutional Neural Networks.md:text/plain},
