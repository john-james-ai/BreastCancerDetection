	author = {Baccouche, Asma and Garcia-Zapirain, Begonya and Castillo Olea, Cristian and S. Elmaghraby, Adel},
	year = {2021},
	keywords = {BCD Screen I, Include, CBIS, BCD Screen I - Include, CBIS-DDSM},
	pages = {1407--1425},
	file = {Baccouche et al. - 2021 - Breast Lesions Detection and Classification via YO.pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\KZZWUE5A\\Baccouche et al. - 2021 - Breast Lesions Detection and Classification via YO.pdf:application/pdf;Breast Lesions Detection and Classification via YOLO-Based Fusion Models.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\Breast Lesions Detection and Classification via YOLO-Based Fusion Models.md:text/plain;Breast Lesions Detection and Classification via YOLO-Based Fusion Models.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\Breast Lesions Detection and Classification via YOLO-Based Fusion Models.md:text/plain},
