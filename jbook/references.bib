	keywords = {BCD Screen I - Include, CBIS-DDSM},
	pages = {19257--19281},
	file = {A computer-aided diagnostic system for mammograms based on YOLOv3.md:C\:\\Users\\John\\iCloudDrive\\iCloud~md~obsidian\\AIStudio\\references\\A computer-aided diagnostic system for mammograms based on YOLOv3.md:text/plain;A computer-aided diagnostic system for mammograms based on YOLOv3.md:C\:\\Users\\John\\Documents\\Knowledge Centre\\Obsidian\\Data Science\\references\\A computer-aided diagnostic system for mammograms based on YOLOv3.md:text/plain;Zhao et al. - 2022 - A computer-aided diagnostic system for mammograms .pdf:C\:\\Users\\John\\Documents\\Knowledge Centre\\Zotero\\storage\\6WUXX8B3\\Zhao et al. - 2022 - A computer-aided diagnostic system for mammograms .pdf:application/pdf},
